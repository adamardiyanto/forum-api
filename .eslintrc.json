{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-undef" :"off",
        "no-underscore-dangle" :"off",
        "class-methods-use-this" :"off",
        "no-unused-vars":"off",
        "import/no-extraneous-dependencies":"off",
        "max-len": "off"
    }
}